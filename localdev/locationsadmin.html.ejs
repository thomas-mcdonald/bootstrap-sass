        else if (slideoutType == 'location') {$("#location-slideout").show(); $("#add-slideout").hide(); }
        else { $("#add-slideout").hide(); $("#location-slideout").hide(); }
      }
      function slidein() {
        $("#slideout").removeClass("active");
